@model BugTracker.Models.ViewModels.DashboardViewModel
@using BugTracker.Services
@using BugTracker.Models
    @{
        ViewData["Title"] = "Dashboard";
    }

    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">
                        Hello  @Model.CurrentUser.FullName
                    </h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Dashboard</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <!-- Info boxes -->
            <div class="row">
                <div class="col-12 col-sm-6 col-md-3">
                    <div class="info-box">
                        <span class="info-box-icon bg-info elevation-1"><i class="fa fa-folder-open" aria-hidden="true"></i></span>

                        <div class="info-box-content">
                            <span class="info-box-text">Active Projects</span>
                            <span class="info-box-number">
                                @Model.Projects.Count
                            </span>
                        </div>
                        <!-- /.info-box-content -->
                    </div>
                    <!-- /.info-box -->
                </div>
                <!-- /.col -->
                <div class="col-12 col-sm-6 col-md-3">
                    <div class="info-box mb-3">
                        <span class="info-box-icon bg-danger elevation-1"><i class="fa fa-ticket" aria-hidden="true"></i></span>

                        <div class="info-box-content">
                            <span class="info-box-text">Total Tickets</span>
                            <span class="info-box-number">@Model.Tickets.Count</span>
                        </div>
                        <!-- /.info-box-content -->
                    </div>
                    <!-- /.info-box -->
                </div>
                <!-- /.col -->
                <!-- fix for small devices only -->
                <div class="clearfix hidden-md-up"></div>

                <div class="col-12 col-sm-6 col-md-3">
                    <div class="info-box mb-3">
                        <span class="info-box-icon bg-success elevation-1"><i class="fa fa-ticket" aria-hidden="true"></i></span>

                        <div class="info-box-content">
                            <span class="info-box-text">Unassigned Tickets</span>
                            <span class="info-box-number">@Model.UnassignedTickets</span>
                        </div>
                        <!-- /.info-box-content -->
                    </div>
                    <!-- /.info-box -->
                </div>
                <!-- /.col -->
                <div class="col-12 col-sm-6 col-md-3">
                    <div class="info-box mb-3">
                        <span class="info-box-icon bg-warning elevation-1"><i class="fas fa-users"></i></span>

                        <div class="info-box-content">
                            <span class="info-box-text">Members</span>
                            <span class="info-box-number">@Model.Members.Count</span>
                        </div>
                        <!-- /.info-box-content -->
                    </div>
                    <!-- /.info-box -->
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-md-3">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Company Data</h3>
                        </div>
                        <div class="card-body">
                            <div class="info info-box-content">New Users</div>
                            <hr />
                            <div class="info info-box-content">Total Users <p class=float-right>@Model.Members.Count</p> </div>
                            <hr />
                            <div class="info info-box-content">Tickets in Development<p class=float-right>@Model.DevelopmentTickets</p></div>
                            <hr />
                            <div class="info info-box-content">Total Developers<p class=float-right>@Model.Developers</p></div>
                        </div>
                        <div class="card-footer"></div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Company Data</h3><br />
                            <h3 class="card-title">@Model.CurrentUser.CompanyId</h3>
                        </div>
                        <div class="card-body">
                            <div class="info info-box-content">Members <p class=float-right>@Model.Members.Count</p></div>
                            <hr />
                            <div class="info info-box-content">Projects <p class=float-right>@Model.Projects.Count</p></div>
                            <hr />
                            <div class="info info-box-content">Tickets <p class=float-right>@Model.Tickets.Count</p></div>
                            <hr />
                            <div class="info info-box-content">Notifications</div>
                        </div>
                        <div class="card-footer"></div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Ticketss By Prioirty</h3>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-8">
                                    <div class="chart-responsive">
                                        <canvas id="pieChart" height="150"></canvas>
                                    </div>
                                    <!-- ./chart-responsive -->
                                </div>
                                <!-- /.col -->
                                <div class="col-md-4">
                                    <ul class="chart-legend clearfix">
                                        <li><i class="far fa-circle text-danger"></i>Low</li>
                                        <li><i class="far fa-circle text-success"></i> Medium</li>
                                        <li><i class="far fa-circle text-warning"></i> High</li>
                                        <li><i class="far fa-circle text-warning"></i> Urgent</li>
                                    </ul>
                                </div>
                                <!-- /.col -->
                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- /.card-body -->
                        <div class="card-footer bg-light p-0">
                            <ul class="nav nav-pills flex-column">
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        Low Priority
                                        <span class="float-right text-danger">
                                            <i class="fas fa-arrow-down text-sm"></i>
                                            @Model.LowPriority
                                        </span>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        Medium Priority
                                        <span class="float-right text-success">
                                            <i class="fas fa-arrow-up text-sm"></i>
                                            @Model.MediumPriority
                                        </span>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        High Priority
                                        <span class="float-right text-warning">
                                            <i class="fas fa-arrow-left text-sm"></i>
                                            @Model.HighPriority
                                        </span>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        Urgent Priority
                                        <span class="float-right text-warning">
                                            <i class="fas fa-arrow-left text-sm"></i>
                                            @Model.UrgentPriority
                                        </span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <!-- /.footer -->
                    </div>
                </div>
                <div class="col-md-3">
                    <!-- STACKED BAR CHART -->
                    <div class="card card-success">
                        <div class="card-header">
                            <h3 class="card-title">Tickets By Type</h3>
                        </div>
                        <div class="card-body">
                            <div class="chart">
                                <canvas id="barChart" style="min-height: 250px; height: 250px; max-height: 250px; max-width: 100%;"></canvas>
                            </div>
                        </div>
                        <!-- /.card-body -->
                    </div>
                    <!-- /.card -->
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
